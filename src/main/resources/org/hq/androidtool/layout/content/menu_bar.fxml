<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="-Infinity" minHeight="500.0" prefHeight="1000.0" prefWidth="250.0" styleClass="menu_bar" stylesheets="@../../style/style_menu_bar.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hq.androidtool.gui.menu_bar_controller">
   <children>
      <VBox prefHeight="0.0" prefWidth="180.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="1.0">
               <children>
                  <ImageView fitHeight="49.0" fitWidth="46.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icon/Interface-icons/mobile_icon_128.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="device_state" alignment="CENTER" prefHeight="20.0" prefWidth="92.0" text="Sin dispositivo" />
                  <Button fx:id="app_reload_state" mnemonicParsing="false" onAction="#onApp_reload_state" prefHeight="27.0" prefWidth="30.0" styleClass="menu_bar_button">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="icon_menu_bar">
                           <image>
                              <Image url="@../icon/Interface-icons/rotate_left_icon_32.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <Button fx:id="app_settings" mnemonicParsing="false" prefHeight="27.0" prefWidth="30.0" styleClass="menu_bar_button">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="icon_menu_bar">
                           <image>
                              <Image url="@../icon/Interface-icons/setting_icon_32.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="app_info" mnemonicParsing="false" onAction="#onInfo_device" prefHeight="20.0" prefWidth="25.0" styleClass="menu_bar_button">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="icon_menu_bar">
                           <image>
                              <Image url="@../icon/Interface-icons/info_circle_icon_32.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </VBox>
      <VBox minHeight="100.0" prefHeight="1000.0" prefWidth="230.0" styleClass="menu_bar_opc">
         <children>
            <HBox fx:id="mb_files" alignment="CENTER_LEFT" onMouseClicked="#onFile_menu_bar" prefHeight="34.0" prefWidth="298.0" styleClass="menu_opc">
               <children>
                  <ImageView pickOnBounds="true" preserveRatio="true" styleClass="icon_menu_bar">
                     <image>
                        <Image url="@../icon/Interface-icons/folder_icon_32.png" />
                     </image>
                  </ImageView>
                  <Label text="Archivos" />
               </children>
            </HBox>
            <HBox fx:id="mb_apps" alignment="CENTER_LEFT" layoutX="10.0" layoutY="21.0" prefHeight="34.0" prefWidth="298.0" styleClass="menu_opc">
               <children>
                  <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" styleClass="icon_menu_bar">
                     <image>
                        <Image url="@../icon/Interface-icons/element_icon_32.png" />
                     </image>
                  </ImageView>
                  <Label text="Aplicaciones" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="78.0" prefHeight="34.0" prefWidth="298.0" styleClass="menu_opc">
               <children>
                  <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" styleClass="icon_menu_bar">
                     <image>
                        <Image url="@../icon/Interface-icons/call_icon_32.png" />
                     </image>
                  </ImageView>
                  <Label text="Contactos" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox prefHeight="61.0" prefWidth="278.0">
         <children>
            <Label styleClass="powered_by" text="Powered by: @hugoquinn" />
            <Region prefHeight="17.0" prefWidth="4.0" HBox.hgrow="ALWAYS" />
            <Label fx:id="lbl_app_version" styleClass="powered_by" text="vx.x" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</VBox>
